<h1>Search Books</h1>
<form action="/books" method="GET">
    <div>
        <label for="">Title</label>
        <input type="text" name="title" id="" value="<%= searchOptions.title  %>">
    </div>
    <br>


    <div>
        <label for="">Published After</label>
        <input type="date" name="publishedAfter" id="" value="<%= searchOptions.publishedAfter  %>">
    </div>
    <br>
    <div>
        <label for="">Published Before</label>
        <input type="date" name="publishedBefore" id="" value="<%= searchOptions.publishedBefore  %>">
    </div>
    <br>
    <div>
        <button type="submit">Search</button>
    </div>
</form>

<div>
    <% books.forEach(book => { %>
<img src="<%= book.coverImagePath  %>" height="150" width="100" alt="">
        
   <% });  %> 
</div>